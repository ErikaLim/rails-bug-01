<!DOCTYPE html>
<html>
<head>
  <title>Rails Practice</title>
  <%= stylesheet_link_tag 'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>
<header>
  <div class="container">
    <div class="logo">
      <%= link_to "Rails Practice", root_path %>
    </div>
     <% if current_user %>
  
        <%= current_user.email %> |


      <%= link_to "Sign Out", signout_path %>
    <% else %>
      <%= link_to "Sign Up", signup_path %>
      |
      <%= link_to "Sign In", signin_path %>
    <% end %>
    <div>

</div>
  </div>
</header>
<main>
  <div class="container">
    <%= yield %>
  </div>
</main>
</body>
</html>
